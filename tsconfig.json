{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "build/tmp",
    "removeComments": false,
    "sourceMap": true,
    "target": "ES5",
    "allowJs": true,
    "keyofStringsOnly": true
  },
  "exclude": [
    "build",
    "target",
    "node_modules"
  ]
}
